  
{
    "name": "web-waste",
    "version": "1.0",
    "manifest_version": 2,
    "permissions": [
      "tabs",
      "activeTab",
      "storage"
    ],
    "content_scripts": [
        {
            "matches": ["http://youtube.com/*"],
            "js":  ["js/content.js"]
        }
    ],
    "browser_action": {
        "default_icon": {
            "16": "img/logo16.png"
        },
        "default_popup": "popup.html",
        "default_title": "Carbon Centric Browsing"
    }
}
